{
  "manifest_version": 2,
  "homepage_url": "https://github.com/transedward/facebook-marks",
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": ["content/index.js"]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": ["background/index.js"]
  },
  "permissions": [
    "bookmarks",
    "https://www.facebook.com/*",
    "http://www.facebook.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
